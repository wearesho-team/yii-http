{
  "name": "wearesho-team/yii-http",
  "type": "proprietary",
  "keywords": [
    "wearesho",
    "yii",
    "http"
  ],
  "require": {
    "php": ">=7.1",
    "yiisoft/yii2": "~2.0.13",
    "horat1us/yii2-base": "^1.8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "squizlabs/php_codesniffer": "^3.3",
    "vlucas/phpdotenv": "^3.3",
    "yiisoft/yii2-phpunit": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Wearesho\\Yii\\Http\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wearesho\\Yii\\Http\\Tests\\": "tests/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Letnikow",
      "email": "reclamme@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "config": {
    "process-timeout": 1800,
    "sort-packages": true,
    "platform": {
      "php": "7.1"
    }
  },
  "scripts": {
    "lint": "phpcs --standard=PSR2 ./src ./tests",
    "phpcbf": "phpcbf --standard=PSR2 ./src ./tests",
    "cover": "phpunit --coverage-clover=coverage.xml",
    "test": "phpunit"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:Horat1us/yii2-phpunit.git"
    }
  ]
}
