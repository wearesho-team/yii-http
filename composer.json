{
  "name": "wearesho-team/yii-http",
  "type": "proprietary",
  "keywords": [
    "wearesho",
    "yii",
    "http"
  ],
  "require": {
    "php": ">=7.1",
    "yiisoft/yii2": "~2.0.13",
    "horat1us/yii2-base": "^1.8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "Wearesho\\Yii\\Http\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wearesho\\Yii\\Http\\Tests\\": "tests/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Letnikow",
      "email": "reclamme@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "process-timeout": 1800,
    "platform": {
      "php": "7.1"
    }
  }
}
